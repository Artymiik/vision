{
	"name": "@artemiik/vision-ui",
	"description": "Keep up to date with what is happening on your server using - Vision",
	"private": false,
	"version": "1.0.2",
	"type": "module",
	"main": "./dist/vision.bundle.js",
	"unpkg": "./dist/vision.bundle.js",
	"homepage": "https://github.com/noneandundefined/vision",
	"repository": "git@github.com:noneandundefined/vision.git",
	"files": [
		"./dist/vision.bundle.js",
		"./dist/vision.css"
	],
	"keywords": [
		"vision",
		"vision-js",
		"vision-ui",
		"monitoring",
		"metrics",
		"load-balancer",
		"stats",
		"server"
	],
	"author": "Artemiik",
	"license": "MIT",
	"scripts": {
		"dev": "vite",
		"install:prod": "set NODE_ENV=production&& pnpm install",
		"build": "tsc -b && vite build",
		"clean": "rimraf ./dist",
		"open-static": "start http://localhost:3000",
		"serve-static": "serve dist",
		"start": "npm-run-all --parallel serve-static open-static",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"preview": "vite preview"
	},
	"dependencies": {
		"axios": "^1.7.9",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-router-dom": "^7.1.5"
	},
	"devDependencies": {
		"@eslint/js": "^9.19.0",
		"@types/axios": "^0.14.4",
		"@types/react": "^19.0.8",
		"@types/react-dom": "^19.0.3",
		"@vitejs/plugin-react-swc": "^3.5.0",
		"autoprefixer": "^10.4.20",
		"esbuild": "^0.25.0",
		"eslint": "^9.19.0",
		"eslint-plugin-react-hooks": "^5.0.0",
		"eslint-plugin-react-refresh": "^0.4.18",
		"globals": "^15.14.0",
		"husky": "^9.1.7",
		"open": "^10.1.0",
		"postcss": "^8.5.2",
		"prettier": "^3.5.1",
		"tailwindcss": "3",
		"typescript": "~5.7.2",
		"typescript-eslint": "^8.22.0",
		"vite": "^6.1.0"
	}
}
